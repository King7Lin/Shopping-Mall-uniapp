
page {
  display: flex;
  flex-direction: column;
  height: 100%;
  /* padding-top: 30rpx; */
}
.van-notice-bar{
	margin-bottom: 20rpx;
}
.top{
	display: flex;
	margin: 0rpx 20rpx 10rpx 20rpx;
}
.number{
	padding-left: 30rpx;
	color: #848484;
}
.address{
	width: 78%;
	padding-left: 20rpx;
	padding-bottom: 20rpx;
	border-bottom: 1px solid #666;
}
.default{
	position: relative;
	top: -55rpx;
	left: 300rpx;
}
.delete-button{
  height: 70%;
		display: flex;
		justify-content: center;
		text-align: center;
		line-height: var(--button-line-height,55px);
		font-size: var(--button-normal-font-size,14px);
		color: #fff;
		background-color: red;
}
.default-button{
	height: 70%;
				display: flex;
				justify-content: center;
				text-align: center;
				line-height: var(--button-line-height,55px);
				font-size: var(--button-normal-font-size,14px);
				color: #fff;
				background-color: limegreen;
}
.add-address {
  margin: 0 auto;
  margin-top: 30px;
  width: 150px;
  height: 35px;
  border: 1px #000 solid;
  line-height: 35px;
  text-align: center;
  color: #000;
  border-radius: 5rpx;
  display: block;
}


